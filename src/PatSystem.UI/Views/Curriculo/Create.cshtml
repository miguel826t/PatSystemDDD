        </div>

        <div class="card mb-4 py-1 border-bottom-primary">
            <div class="w3-bar w3-white active py-1" role="tab" id="id02" data-toggle="collapse" data-target="#c02">
                <button onclick="document.getElementById('ModalExperiencia').style.display='block'" type="button" class="w3-bar-item w3-button w3-left w3-hover-none pr-0 pt-2"><i class="fas fa-plus-square mr-1"></i></button>
                <h6 class="w3-bar-item m-0 font-weight-bold text-primary pl-1">Idioma</h6>
                <button type="button" class="w3-bar-item w3-button w3-left w3-hover-none w3-hover-text-blue"><i class="fas fa-sort"></i></button>
            </div>
            <table class="w3-table table-bordered w3-small w3-table-all w3-striped w3-border w3-white">
                @if (Model.Experiencias != null)
                {
                    <thead>
                        <tr>
                            <th class="w3-center">Codigo</th>
                            <th class="w3-left-align">Empresa</th>
                            <th class="w3-center">Cargo</th>
                            <th class="w3-center">Anos</th>
                            <th class="w3-center">Descricao</th>
                            <th class="w3-center">Remover</th>
                        </tr>
                    </thead>
                    <tbody>
                        @for (var i = 0; i < Model.Experiencias.Count; i++)
                        {
                            <tr>
                                <td class="w3-center">@Model.Experiencias[i].ExperienciaId<input asp-for="Experiencias[i].ExperienciaId" readonly style="border:0px;background:none;" class="w3-center" value="@Model.Experiencias[i].ExperienciaId" type="hidden" /></td>
                                <td class="w3-center"><input asp-for="Experiencias[i].NomeEmpresa" readonly style="border:0px;background:none;" class="w3-center" value="@Model.Experiencias[i].NomeEmpresa" /></td>
                                <td class="w3-center"><input asp-for="Experiencias[i].UltimoCargo" readonly style="border:0px;background:none;" class="w3-center" value="@Model.Experiencias[i].UltimoCargo" /></td>
                                <td class="w3-center"><input asp-for="Experiencias[i].Anos" readonly style="border:0px;background:none;" class="w3-center" value="@Model.Experiencias[i].Anos" /></td>
                                <td class="w3-center"><input asp-for="Experiencias[i].Descricao" readonly style="border:0px;background:none;" class="w3-center" value="@Model.Experiencias[i].Descricao" /></td>
                                <td class="w3-center" style="padding:0px;"><a class="btn w3-hover-red btn-sm" style="margin-top:3px;"><i class="far fa-trash-alt"></i></a></td>
                            </tr>
                        }
                    </tbody>
                }
            </table>
        </div>

    </nav>



    <div class="form-group row mt-2">
        <a asp-action="Index" class="btn btn-primary mr-2">Voltar</a>
        <input asp-action="CreateCR" class="btn btn-success  pr-3" type="submit" value="Criar" btn btn-primary" />
    </div>
</form>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
