                    <div class="col-lg-4 col-md-4 col-12">
                        <label asp-for="Cliente.EnsinoMedio" class="mb-1 mr-sm-2"></label>
                        <select asp-for="Cliente.EnsinoMedio" class="custom-select form-control m-0 mr-sm-2">
                            <option>  </option>
                            <option>Incompleto</option>
                            <option>Completo</option>
                            <option>Cursando</option>
                        </select>
                    </div>
                </div>
            </div>
        </div>
