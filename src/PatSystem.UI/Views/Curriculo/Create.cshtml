@model PatSystem.UI.ViewModels.CRcreateViewModel

@{
    ViewData["Title"] = "Criar";
}
<script src="~/script/ScriptBuscaCidade.js"></script>
<script src="~/script/ScriptCursoTecnico.js"></script>
<script src="~/script/ScriptCursoSuperior.js"></script>
@await Html.PartialAsync("~/Views/Curriculo/PartialViews/_MenuSuperior.cshtml")
<hr />

@if (Model.Erros != null)
{
    <div class="col-12 mt-4">
        <div class="alert alert-danger alert-dismissible">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            @Html.DisplayFor(model => model.Erros)
        </div>
    </div>
}

<form asp-controller="Curriculo" style="font-size:15px">
